@model GhostRunner.ViewModels.Sequences.Partials.SequenceScriptsModel

@if (Model.SequenceScripts.Count > 0)
{
    foreach (SequenceScript script in Model.SequenceScripts)
    {
        <div id="script_@script.ExternalId" class="script sequence-script" script-id="@script.ExternalId">
            <div>@script.Name</div>
            <div class="script-type">@ScriptHelper.GetReadableScriptType(script.Type.ToString())</div>
        </div>
    }
}
else
{
    <div style="text-align: center">...</div>
}