  <applicationSettings>
    <GhostRunner.Properties.Settings>
      <setting name="AllowAccountCreate" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="PackageCacheLocation" serializeAs="String">
        <value>D:\PackageCache</value>
      </setting>
    </GhostRunner.Properties.Settings>
